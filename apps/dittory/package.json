{
  "name": "dittory",
  "version": "0.0.5",
  "description": "A static analysis CLI that detects parameters always receiving the same value in React components and functions",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    }
  },
  "bin": {
    "dittory": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown && node ./dist/cli.mjs",
    "lint": "biome check --write src",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "cp ../../README.md ./README.md && pnpm run build",
    "knip": "knip --production"
  },
  "keywords": [
    "react",
    "props",
    "analyzer",
    "static-analysis",
    "typescript"
  ],
  "author": "warabi1062",
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "repository": "github:warabi1062/dittory",
  "homepage": "https://warabi1062.github.io/dittory/",
  "packageManager": "pnpm@10.6.4",
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "knip": "^5.80.2",
    "react": "^19.2.3",
    "tsdown": "^0.18.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "ts-morph": "^27.0.2"
  }
}
